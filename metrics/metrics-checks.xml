<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Puppy Crawl//DTD Check Configuration 1.3//EN" "http://www.puppycrawl.com/dtds/configuration_1_3.dtd">

<module name="Checker">
	<property name="severity" value="warning"/>
	<module name="TreeWalker">
		<module name="BooleanExpressionComplexity">
			<property name="severity" value="info"/>
			<property name="max" value="0"/>
			<message key="booleanExpressionComplexity" value="type:bool_expression val:{0,number,integer}"/>
		</module>
		<module name="ClassDataAbstractionCoupling">
			<property name="severity" value="info"/>
			<property name="max" value="0"/>
			<message key="classDataAbstractionCoupling" value="type:CDAC val:{0,number,integer} classes {2}."/>
		</module>
		<module name="ClassFanOutComplexity">
			<property name="severity" value="info"/>
			<property name="max" value="0"/>
			<message key="classFanOutComplexity" value="type:CFOC val:{0,number,integer}"/>
		</module>
		<module name="CyclomaticComplexity">
			<property name="severity" value="info"/>
			<property name="max" value="0"/>
			<message key="cyclomaticComplexity" value="type:CYC val:{0,number,integer}"/>
		</module>
		<module name="JavaNCSS">
			<property name="severity" value="info"/>
			<property name="methodMaximum" value="0"/>
			<property name="classMaximum" value="0"/>
			<property name="fileMaximum" value="0"/>
			<message key="ncss.method" value="type:NCSS_method val:{0,number,integer}"/>
			<message key="ncss.class" value="type:NCSS_class val:{0,number,integer}"/>
			<message key="ncss.file" value="type:NCSS_file val:{0,number,integer}"/>
		</module>
		<module name="NPathComplexity">
			<property name="severity" value="info"/>
			<property name="max" value="0"/>
			<message key="npathComplexity" value="type:NPath val:{0,number,integer}"/>
		</module>
	</module>
</module>
